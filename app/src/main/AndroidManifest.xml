<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.account_book">

    <application
        android:allowBackup="true"
        android:icon="@drawable/loading_img"
        android:label="푸른 가계부"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Account_book">
        <activity android:name=".ListActivity"></activity>
        <activity android:name=".MemoAddActivity" />
        <activity android:name=".MemoActivity" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".Success" />
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Information" />
    </application>

</manifest>